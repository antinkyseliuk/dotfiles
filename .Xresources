Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

xterm*faceName: Inconsolata
xterm*faceSize: 11
xterm*borderWidth: 0

urxvt*saveLines: 5000
urxvt*scrollBar: false
urxvt*internalBorder: 10

urxvt*font: xft:Inconsolata:size=11,xft:Inconsolata\ For\ Powerline:size=11
urxvt*boldFont: xft:Inconsolata:bold:size=11
urxvt*italicFont: xft:Inconsolata:italic:size=11
urxvt*bolditalicFont: xft:Inconsolata:bolditalic:size=11

!urxvt.perl-ext-common: clipboard
!urxvt*keysym.M-C-c: perl:clipboard:copy
!urxvt*keysym.M-v: perl:clipboard:paste
!urxvt*keysym.M-C-v: perl:clipboard:paste_escaped

xterm*selectToClipboard: true

! special
*.foreground:   #F5F6F7
*.background:   #414A59
*.cursorColor:  #5294E2

! black
*.color0:       #20201D
*.color8:       #7D7A68

! red
*.color1:       #CC575D
*.color9:       #CC575D

! green
*.color2:       #60AC39
*.color10:      #60AC39

! yellow
*.color3:       #CFB017
*.color11:      #CFB017

! blue (bold)
*.color4:       #5294E2
*.color12:      #5294E2

! magenta
*.color5:       #D599E6
*.color13:      #D599E6

! cyan
*.color6:       #1FAD83
*.color14:      #1FAD83

! white
*.color7:       #A6A28C
*.color15:      #F5F6F7

